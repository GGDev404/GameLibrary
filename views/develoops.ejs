<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/static/src/styles/stylehome.css">
    <link rel="stylesheet" href="/static/src/styles/styledeveloops.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.rtl.min.css" 
    integrity="sha384-PJsj/BTMqILvmcej7ulplguok8ag4xFTPryRq8xevL7eBYSmpXKcbNVuy+P0RMgq" crossorigin="anonymous">
    <link href='https://unpkg.com/boxicons@2.1.4/css/boxicons.min.css' rel='stylesheet'>
    <title>Game Library</title>
</head>
<body>
    <div class="page-wrapper">
        <div class="nav-wrapper">
            <div class="grad-bar"></div>
            <nav class="navbar">
                <img src="/static/src/img/brin.png" alt="Company Logo">
                <div class="menu-toggle" id="mobile-menu">
                    <span class="bar"></span>
                    <span class="bar"></span>
                    <span class="bar"></span>
                </div>
                <ul class="nav no-search">
                    <li class="nav-item" style="margin-left: 20px;"><a href="home">Home</a></li>
                    <li class="nav-item" style="margin-right: 10px;"><a href="/Games">Videogames</a></li>
                    <li class="nav-item"><a href="/dev">Develoops</a></li>
                    <li class="nav-item"><a href="/genders">Genders</a></li>
                </ul>
            </nav>
            </div>
            <!-- /*--------------------------------------------------------------------------------------------------------------------------------------*/ -->
            <div class="juegoscrud">
                <div class="container-homecrud">
                    <% results.forEach((results) => { %>
                        <div class="card">
                            <img src="<%= results.img %>" class="card-img" alt="">
                            <div class="card-body">
                                <h1 class="card-title"><%= results.nombre %></h1>
                                <p class="card-sub-title"><%= results.pais %></p>
                                <p class="card-info"><%= results.fundacion %></p>
                                <a href="/editDev/<%= results.id_desarrolladora %>" class="btn btn-outline-info">editar</a>
                                <a href="/deleteDev/<%= results.id_desarrolladora %>" class="btn btn-outline-danger">borrar</a>
                            </div>
                        </div>
                        <% }) %>
                </div>
            </div>
            
            <!-- /*--------------------------------------------------------------------------------------------------------------------------------------*/ -->
            <!-- /*--------------------------------------------------------------------------------------------------------------------------------------*/ -->
            <div class="agregar">
                <a href="/addGame">
                    <button class="bb"><span></span>Add</button>
                </a>
            </div>
        </div>
    </div>
    <!-- /*--------------------------------------------------------------------------------------------------------------------------------------*/ -->
</body>

</html>